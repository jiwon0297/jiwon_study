   "source": [
    "# code up 1904 : 두 수의 홀수 출력하기\n",
    "# 메모리 33764 시간 26\n",
    "\n",
    "a,b=map(int,input().split())\n",
    "\n",
    "def fn(a,b):\n",
    "    if a>b:\n",
    "        return 0\n",
    "    elif b%2==0: #b가 짝수일 때\n",
    "        fn(a,b-1) #출력 없이 b-1\n",
    "    elif b%2!=0: #b가 홀수일 때\n",
    "        fn(a,b-1)\n",
    "        print(b,end=' ') #출력\n",
    "\n",
    "fn(a,b)"
   ]
  }
